import pandas as pd
import matplotlib.pyplot as plt

# Read the CSV file
data = pd.read_csv('drug_response.csv')

# Plot each Value column versus Sample
for col in data.columns[1:]:
    plt.plot(data['Sample'], data[col], marker='o', label=col)

plt.xlabel('Sample')
plt.ylabel('Response')
plt.title('Drug Response')
plt.legend()
plt.tight_layout()
plt.savefig('drug_response_plot.png')
plt.show()
